[system]
    debug = true
    response_type = 'map'
    use_cache = true

[system.log]
    level = 'debug'
    file = 'logs/wechat.log'

[route]
        [route.open_platform]
            uri=  '/server'

[domain]
        [domain.host]
            url = ''
        [domain.default]
            url = 'https://api.mch.weixin.qq.com'
        [domain.official_account]
#通用域名(api.weixin.qq.com)，使用该域名将访问官方指定就近的接入点；
#上海域名(sh.api.weixin.qq.com)，使用该域名将访问上海的接入点；
#深圳域名(sz.api.weixin.qq.com)，使用该域名将访问深圳的接入点；
#香港域名(hk.api.weixin.qq.com)，使用该域名将访问香港的接入点。
#            url = 'https://api.weixin.qq.com'

[official_account]
#        [official_account.config]
#            data_type = "json"

        [official_account.default]
            app_id =''                    #AppID
            secret = ''     #AppSecret
            token = ''                            #Token
            aes_key = ''    #EncodingAESKey

        [official_account.oauth]
            scopes   = ['snsapi_userinfo']
            callback = '/oauth_callback'


[open_platform]
        [open_platform.default]
            app_id =''                      #AppID
            secret = ''       #AppSecret
            token = 'your-token'                              #Token
            aes_key = ''      #EncodingAESKey

[mini_program]
        [mini_program.default]
            app_id =''                      #AppID
            secret = ''       #AppSecret
            token = 'your-token'                              #Token
            aes_key = ''      #EncodingAESKey

[payment]
        [payment.default]
            sandbox = false
            app_id =''
            mch_id = ''
            key = ''
            notify_url ='wego/notify'
            cert_path = 'cert/apiclient_cert.pem'
            key_path = 'cert/apiclient_key.pem'
            rootca_path = 'cert/rootca.pem'
            pubkey_path = "rsa/publickey.pem"
            prikey_path = "rsa/privatekey.pem"

[work]
        [work.default]
            corp_id = ''
            agent_id = 100020
            secret  = ''
